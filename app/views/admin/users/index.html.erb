<h1>ユーザー一覧</h1>
<%= link_to 'ユーザー登録',new_admin_user_path %>
<br>
<%= notice %>
<table class="table">
  <thead>
    <tr>
      <th>名前</th>
      <th>メールアドレス</th>
      <th>作成日時</th>
      <th>編集日時</th>
      <th>管理者登録</th>
      <th>ユーザー編集</th>
      <td>ユーザー詳細</td>
      <td>ユーザー削除</td>
    </tr>
  <thead>
  <tbody>
    <% @users.each do |user|%>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.created_at %></td>
      <td><%= user.updated_at %></td>
      <td><%= user.admin? ? 'あり' : 'なし' %></td>
      <td><%= link_to '編集',edit_admin_user_path(@users.ids) %></td>
      <td><%= link_to '詳細',admin_user_path(@users.ids) %></td>
      <td><%= link_to '削除',admin_user_path(@users.ids),method: :delete %></td>
    <% end %>
    </tr>
  </tbody>
</table>